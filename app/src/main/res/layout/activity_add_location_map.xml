<?xml version="1.0" encoding="utf-8"?>


<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:map="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"

    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"

    tools:context=".AddLocationMapActivity">

    <fragment
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/map"
        android:name="com.google.android.gms.maps.SupportMapFragment"/>

    <Button
        android:id="@+id/bt_onFinishAddLocationClick"
        android:layout_width="114dp"
        android:layout_height="67dp"
        android:layout_weight="1"
        android:backgroundTint="#FAF8F8"

        android:onClick="onFinishAddLocationClick"
        android:text="Finish"
        android:textColor="#9754CB"
        android:textSize="16sp"

        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        map:layout_constraintHorizontal_bias="0.946"
        map:layout_constraintVertical_bias="0.975" />

</androidx.constraintlayout.widget.ConstraintLayout>



